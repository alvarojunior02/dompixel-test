generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Product {
  id         Int      @id @default(autoincrement())
  name       String   @unique
  category   String
  price      Decimal
  created_at DateTime @default(now())

  @@map("products")
}
